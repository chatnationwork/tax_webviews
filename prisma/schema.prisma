// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SavedItem {
  id            Int      @id @default(autoincrement())
  category      String?
  item          String?
  quantity      String?
  amount        String?
  currency      String?
  valueOfFund   String?
  sourceOfFund  String?
  purposeOfFund String?
  cert          String?
  hsCode        String?
  phone         String?
  make          String?
  model         String?
  imei          String?
  file          String[]
  createdAt     DateTime @default(now())
}

model visitedCountires {
  id        Int      @id @default(autoincrement())
  country   Json
  phone     String?
  createdAt DateTime @default(now())
}
